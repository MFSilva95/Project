<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal" >
    
	<LinearLayout
	    android:id="@+id/ll_list_exercisereg_left"
	    android:layout_width="wrap_content"
	    android:layout_height="48dp"
	    android:layout_marginLeft="3dp"
	    android:orientation="vertical"
	    android:layout_centerVertical="true"
	    android:layout_alignParentLeft="true" >
	    <TextView
	        android:id="@+id/tv_list_exercisereg_data"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:layout_weight="1"
	        android:gravity="center_horizontal"
	        android:textSize="16sp" />
        <TextView
	        android:id="@+id/tv_list_exercisereg_hora"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:layout_weight="1"
	        android:gravity="center_horizontal"
	        android:textSize="16sp" />
        
	</LinearLayout>
	
	<LinearLayout
	    android:id="@+id/ll_list_exercisereg_middleleft"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_marginRight="8dp"
	    android:orientation="vertical"
	    android:layout_centerVertical="true"
	    android:layout_toRightOf="@+id/ll_list_exercisereg_left"
	    android:layout_toLeftOf="@+id/ll_list_exercisereg_middleright" >
	    <TextView
	        android:id="@+id/tv_list_exercisereg_exercise"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:layout_weight="1"
	        android:gravity="center_horizontal"
	        android:textSize="16sp" />
	    <TextView
	        android:id="@+id/tv_list_exercisereg_effort"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:layout_weight="1"
	        android:gravity="center_horizontal"
	        android:textSize="12sp" />
	</LinearLayout>
	
	<LinearLayout
	    android:id="@+id/ll_list_exercisereg_middleright"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_marginRight="8dp"
	    android:orientation="vertical"
	    android:layout_centerVertical="true"
	    android:layout_toLeftOf="@+id/ib_list_exercisereg_detail" >
        <TextView
	        android:id="@+id/tv_list_exercisereg_duration"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:layout_weight="1"
	        android:gravity="center_horizontal"
	        android:textSize="16sp"
	        android:textStyle="bold" />
        <TextView
	        android:id="@+id/tv_list_exercisereg_exercise_minutes"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:layout_weight="1"
	        android:gravity="center_horizontal"
	        android:text="minutos"
	        android:textSize="12sp" />
	</LinearLayout>
	
	<ImageButton
        android:contentDescription="exercise_detail_img"
        android:id="@+id/ib_list_exercisereg_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:paddingRight="8dp"
        android:paddingLeft="8dp"
        android:layout_alignParentRight="true"
        android:src="@drawable/viewdetail"
        android:background="@color/transparent" />
</RelativeLayout><!-- From: file:/C:/Users/ZÃ©/Desktop/Development/Workspaces/Android%20Studio/MyDiabetes/app/src/main/res/layout/list_exercisereg_row.xml -->