<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="bottom|end"
    android:clipChildren="false">


    <LinearLayout
        android:id="@+id/fab_container_h"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:clipChildren="false"
        android:orientation="vertical">

        <android.support.design.widget.FloatingActionButton
            android:id="@+id/mini_fab4"
            android:layout_gravity="center"
            android:layout_margin="@dimen/fab_margin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/diseases"
            android:visibility="visible"
            app:backgroundTint="?colorAccent"
            app:borderWidth="1dp"
            app:elevation="6dp"
            app:fabSize="mini"
            app:rippleColor="?colorPrimaryDark"
            tools:visibility="visible" />

        <android.support.design.widget.FloatingActionButton
            android:id="@+id/mini_fab5"
            android:layout_gravity="center"
            android:layout_margin="@dimen/fab_margin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/cholesterol"
            android:visibility="visible"
            app:backgroundTint="?colorAccent"
            app:borderWidth="1dp"
            app:elevation="6dp"
            app:fabSize="mini"
            app:rippleColor="?colorPrimaryDark"
            tools:visibility="visible" />

        <android.support.design.widget.FloatingActionButton
            android:id="@+id/mini_fab6"
            android:layout_gravity="center"
            android:layout_margin="@dimen/fab_margin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/weight"
            android:visibility="visible"
            app:backgroundTint="?colorAccent"
            app:borderWidth="1dp"
            app:elevation="6dp"
            app:fabSize="mini"
            app:rippleColor="?colorPrimaryDark"
            tools:visibility="visible" />

        <android.support.design.widget.FloatingActionButton
            android:id="@+id/mini_fab7"
            android:layout_gravity="center"
            android:layout_margin="@dimen/fab_margin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/exercise"
            android:visibility="visible"
            app:backgroundTint="?colorAccent"
            app:borderWidth="1dp"
            app:elevation="6dp"
            app:fabSize="mini"
            app:rippleColor="?colorPrimaryDark"
            tools:visibility="visible" />

        <android.support.design.widget.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:layout_gravity="bottom|end"
            android:layout_margin="@dimen/fab_margin"
            app:fabSize="normal"
            android:src="@android:drawable/ic_input_add"
            android:id="@+id/fab"
            android:layout_weight="1" />

    </LinearLayout>



</FrameLayout>